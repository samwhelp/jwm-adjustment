<?xml version="1.0"?>
<JWM>


	<!-- ## IconPath //-->
	<Include>$HOME/.config/jwm/config/main/icon/path/default.conf</Include>


	<!-- ## RootMenu //-->
	<!-- <Include>$HOME/.config/jwm/config/main/menu/root/default.conf</Include> //-->
	<!-- <Include>$HOME/.config/jwm/config/main/menu/root/debian.conf</Include> //-->
	<Include>$HOME/.config/jwm/config/main/menu/root/basic.conf</Include>


	<!-- Options for program groups. //-->
	<Group>
		<Option>tiled</Option>
	</Group>
	<Group>
		<Name>xterm</Name>
		<Option>vmax</Option>
	</Group>
	<Group>
		<Name>xclock</Name>
		<Option>drag</Option>
		<Option>notitle</Option>
	</Group>


	<!-- ## Panel //-->
	<!-- <Include>$HOME/.config/jwm/config/main/panel/top.conf</Include> //-->
	<Include>$HOME/.config/jwm/config/main/panel/bottom.conf</Include>


	<!-- ## Style //-->
	<Include>$HOME/.config/jwm/config/main/style/Default/init.conf</Include>
	<!-- <Include>$HOME/.config/jwm/config/main/style/Default-Flat/init.conf</Include> //-->




	<!-- Virtual Desktops //-->
	<!-- Desktop tags can be contained within Desktops for desktop names. //-->
	<Desktops width="4" height="1">
		<!--
			Default background.
			Note that a Background tag can be contained within a Desktop tag
			to give a specific background for that desktop.
		//-->
		<!-- <Background type="solid">#111111</Background> //-->
		<Background type="image">/usr/share/backgrounds/default.jpg</Background>
	</Desktops>



	<!-- ## Feature //-->
	<Include>$HOME/.config/jwm/config/main/feature/default.conf</Include>


	<!-- ## Keybind //-->
	<Include>$HOME/.config/jwm/config/main/keybind/default.conf</Include>


	<!-- ## Mousebind //-->
	<Include>$HOME/.config/jwm/config/main/mousebind/default.conf</Include>


	<!-- ## Autostart //-->
	<Include>$HOME/.config/jwm/config/main/autostart/default.conf</Include>
	<!-- <Include>$HOME/.config/jwm/config/main/autostart/basic.conf</Include> //-->


</JWM>
